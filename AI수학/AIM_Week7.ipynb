      "source": [
        "import numpy as np"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "A = np.array([[3,6],\n",
        "              [2,3],\n",
        "              [1,2],\n",
        "              [5,5]])\n",
        "print(A)"
      ],
      "metadata": {
        "id": "vChfoVwPWVf7"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "U, sigma, V_T = np.linalg.svd(A)\n",
        "\n",
        "print(U)\n",
        "print(np.diag(sigma))\n",
        "print(V_T)"
      ],
      "metadata": {
        "id": "IQH21sh5W1xS"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "np.matmul(np.matmul(U[:, :2], np.diag(sigma)), V_T)"
      ],
      "metadata": {
        "id": "M2ILKPJsY5cw"
      },
      "execution_count": null,
      "outputs": []
    }
